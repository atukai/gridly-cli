{
    "name": "atukai/gridly-cli",
    "description": "Gridly CLI - command Line Interface for atukai/gridly",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/atukai/gridly-cli",
    "authors": [
        {
            "name": "Alexander Tukai",
            "email": "atukai@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "atukai/gridly": "dev-master",
        "laminas/laminas-paginator": "^2.17",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "src/gridly"
    ],
    "autoload": {
        "psr-4": {
            "GridlyCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GridlyCli\\Test\\": "tests/"
        }
    }
}